{
  "name": "todo",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --port=5200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "lint": "ng lint",
    "serve:ssr": "node dist/todo/server/server.mjs",
    "build:run": "ng build --configuration=production && ng run todo:server:production",
    "vercel:build": "ng build && node dist/todo/server/server.mjs",
    "prerender": "ng run todo:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-server": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@angular/ssr": "^19.1.5",
    "@apollo/client": "^3.12.5",
    "@netlify/angular-runtime": "^2.4.0",
    "@netlify/functions": "^4.1.10",
    "@primeng/themes": "^19.1.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/node": "^5.3.0",
    "@vercel/speed-insights": "^1.2.0",
    "apollo-angular": "^10.0.3",
    "compression": "^1.8.0",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "graphql": "^16.11.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.5",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular/cli": "^19.1.5",
    "@angular/compiler-cli": "^19.1.0",
    "@tailwindcss/postcss": "^4.0.4",
    "@types/compression": "^1.8.1",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.4",
    "@typescript-eslint/parser": "^8.22.0",
    "angular-eslint": "^19.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.18.0"
  }
