{
  "version": 2,
  "public": true,
  "name": "todo",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/todo/**"
    }
  },
  "headers": [
    {
      "key": "Access-Control-Allow-Credentials",
      "value": "true"
    },
    {
      "key": "Access-Control-Expose-Headers",
      "value": "Set-Cookie"
    }
  ]
}