{
  "version": 2,
  "builds": [
    {
      "src": "dist/todo/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/todo/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/dist/todo/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "outputDirectory": "dist/todo/browser"
}