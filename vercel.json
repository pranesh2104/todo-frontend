{
  "version": 2,
  "public": true,
  "name": "todo",
  "framework": "angular",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Access-Control-Expose-Headers",
              "value": "Set-Cookie"
            },
            {
              "key": "Access-Control-Allow-Credentials",
              "value": "true"
            },
            {
              "key": "Cache-Control",
              "value": "no-cache, no-store"
            }
          ]
        }
      ]
    }
  ]
}