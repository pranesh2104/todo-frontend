{
  "version": 2,
  "public": true,
  "name": "todo",
  "framework": "angular",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "headers": {
        "Access-Control-Expose-Headers": "Set-Cookie"
      }
    }
  ]
}