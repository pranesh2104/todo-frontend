{
  "version": 2,
  "public": true,
  "name": "todo",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/todo/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/todo/server/server.mjs",
      "headers": {
        "Set-Cookie": ":original"
      }
    }
  ]
}