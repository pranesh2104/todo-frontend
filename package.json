{
  "name": "todo",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --port=5200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:prod": "ng build --configuration=production",
    "serve:prod:ssr": "node dist/todo/server/server.mjs",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/todo/stats.json",
    "lint": "ng lint",
    "start:ssr": "concurrently 'npm:start:angular' 'npm:start:dev-server'",
    "start:angular": "ng serve --proxy-config proxy.conf.json",
    "start:dev-server": "nodemon",
    "serve:ssr:todo": "node dist/todo/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.5",
    "@angular/common": "^19.1.5",
    "@angular/compiler": "^19.1.5",
    "@angular/core": "^19.1.5",
    "@angular/forms": "^19.1.5",
    "@angular/platform-browser": "^19.1.5",
    "@angular/platform-server": "^19.2.2",
    "@angular/router": "^19.1.5",
    "@angular/ssr": "^19.1.6",
    "@apollo/client": "^3.12.5",
    "@primeng/themes": "^19.1.3",
    "apollo-angular": "^10.0.3",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "graphql": "^16.11.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular/cli": "^19.1.6",
    "@angular/compiler-cli": "^19.1.5",
    "@tailwindcss/postcss": "^4.0.4",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.4",
    "@typescript-eslint/parser": "^8.22.0",
    "angular-eslint": "^19.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.18.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  }
}
